<div class="eventsContainer tdContainer">
{{> backLink}} {{> infoLink}} {{> big-mac}} {{> mobile-menu}}
<div id="testdriveLogoTL">
  <a  href="/testdrive"><img  src="/assets/Logo-SVG/Testdrive.svg"></a>
</div>





  <div class="eventList" id="tdEventsList">
    {{#eventList}}
      <div class="eventDetails">
          <p class="eventsDetailDate" >{{eventDate}}</p>
          <p class="eventsDetailTitle">{{eventTitle}}</p>
      </div>
    {{/eventList}}
  </div>

  <div class="eventsInfoBox eventsInfoBoxClosed" id="tdEventsInfobox">
    {{#eventList}}
      <div class='eventInfo'>
        <h1 class="event-top-title">{{eventTitle}}</h1>
        {{#eventContent}}
          <br></br>
          <img class="events-img" src= {{image}} alt="">
          <p>{{imageDescrip}}</p>
        {{/eventContent}}
        {{{eventInfo}}}
      </div>
    {{/eventList}}
  </div>



</div>
