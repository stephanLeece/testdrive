{{> testdriveEvents }} {{> backLink}}
{{> infoLink}}

<div class="events-menu">
  <div class="eventList" id="tdEventsList">
    {{#eventList}}
      <div class="eventDetails">
        <p>{{eventDate}}</p>
        <p>{{eventTitle}}</p>
      </div>
    {{/eventList}}
  </div>
  <div class="eventsInfoBox" id="tdEventsInfobox">
    {{#eventList}}
    <div class='eventInfo'>
        {{#eventContent}}
          <img src= {{image}} alt="">
          <p>{{imageDescrip}}</p>
        {{/eventContent}}
        {{{eventInfo}}}
      </div>
    {{/eventList}}
  </div>
</div>
