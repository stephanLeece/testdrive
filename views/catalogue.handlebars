{{> big-mac}}
{{> infoLink}}
{{> backLink}}

<div id="cat-disp">
  <p>+catalogue</p>
</div>

<div id="artist-container">
  <div class="artists">
  {{#artists}}
  <ul>{{.}}</ul>
  {{/artists}}
  </div>
</div>
<!-- <img class="artistbackground" src="/assets/Elements/DriveDrive/Catalogue/artist-list-desktop.svg"> -->
<img class="artistbackground" src="/assets/mobile/artist-list-mobile.svg">

<img id="catalogue-line-green" src="/assets/Elements/DriveDrive/Catalogue/DD-Catalogue-line-green.svg">


<div id="pdfs-container">
  <ul class="pds">
  {{#catalogue}}
    <li><a class="pdf" href= 'http://{{{ url }}}' target="_blank">{{fileName}}</a></li>
  {{/catalogue}}
  </ul>
</div>

  <img id="catalogue-line-blue" src="/assets/Elements/DriveDrive/Catalogue/DD-Catalogue-line-blue.svg">

  <div id="video-container">

    <div>
    {{#video}}
    <video  id="video" onmouseover="pause()" onmouseleave="play()" autoplay loop>
      <source src='https:{{url}}' type='{{contentType}}'>
    </video>
    {{/video}}
    </div>
  </div>
